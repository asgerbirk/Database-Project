{
  "name": "database-project",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "vitest",
    "test:unit": "npx vitest test/unit",
    "test:integration": "npx vitest test/integration",
    "migrate": "npx prisma migrate dev --name",
    "pull": "npx prisma db pull",
    "reset": "npx prisma migrate reset --force && npm run cleanup-indexes",
    "lint": "eslint . --ext .ts",
    "seed": "node --loader ts-node/esm src/databases/prisma/seed.ts",
    "coverage": "vitest run --coverage",
    "test:ui": "npx vitest --ui --coverage"
  },
  "prisma": {
    "schema": "./src/databases/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@vitest/coverage-istanbul": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.21.0",
    "nodemon": "^3.1.4",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@eslint/js": "^9.17.0",
    "@prisma/client": "^5.20.0",
    "@types/k6": "^0.54.2",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "bcrypt": "^5.1.1",
    "cookie": "^1.0.2",
    "cookie-parser": "^1.4.7",
    "cookieparser": "^0.1.0",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "csurf": "^1.10.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.17.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.12.0",
    "multer": "^1.4.5-lts.1",
    "neo4j-driver": "^5.26.0",
    "prettier": "^3.4.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "xml2js": "^0.6.2",
    "zod": "^3.23.8"
  }
}
